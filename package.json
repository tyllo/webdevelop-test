{
  "name": "webdevelop-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "build": "vue-cli-service build --mode production",
    "lint": "cross-env NODE_ENV=production yarn lint:eslint && yarn lint:stylelint",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:stylelint --fix",
    "lint:stylelint": "stylelint --cache \"src/**/*.{vue,scss}\"",
    "lint:eslint": "eslint --cache \"src/**/*.{js,ts,vue}\" --ignore-path .gitignore .",
    "prepush": "yarn lint && yarn audit fix --groups dependencies"
  },
  "dependencies": {
    "core-js": "3.9.1",
    "vue": "^2.6.12",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2",
    "vuex-smart-module": "^0.5.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@vue/cli-plugin-babel": "~4.5.12",
    "@vue/cli-plugin-eslint": "~4.5.12",
    "@vue/cli-plugin-router": "~4.5.12",
    "@vue/cli-plugin-typescript": "^4.5.12",
    "@vue/cli-plugin-vuex": "~4.5.12",
    "@vue/cli-service": "~4.5.12",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "husky": "5.1.3",
    "sass": "^1.32.8",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.12.0",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-strict-value": "^1.7.9",
    "stylelint-scss": "^3.19.0",
    "typescript": "^4.2.3",
    "vue-template-compiler": "^2.6.12"
  }
}
